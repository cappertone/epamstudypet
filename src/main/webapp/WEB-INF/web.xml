<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>petServlet</display-name>

    <servlet>
        <servlet-name>DevelopersServlet</servlet-name>
        <servlet-class>com.epam.petproject.rest.DevelopersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SimpleServlet</servlet-name>
        <servlet-class>com.epam.petproject.rest.SimpleServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SimpleServlet</servlet-name>
        <url-pattern>/s</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DevelopersServlet</servlet-name>
        <url-pattern>/developers</url-pattern>
    </servlet-mapping>
</web-app>
